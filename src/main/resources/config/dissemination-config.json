    {
      "prefix": "mets",
      "url": "http://www.loc.gov/METS/"
    },
    {
      "prefix": "mext",
      "url": "http://slub-dresden.de/mets"
    },
    {
      "prefix": "mods",
      "url": "http://www.loc.gov/mods/v3"
    },
    {
      "prefix": "xlin",
      "url": "http://www.w3.org/1999/xlink"
    },
    {
      "prefix": "slub",
      "url": "http://slub-dresden.de/"
    },
    {
      "prefix": "xMetaDiss",
      "url": "http://www.d-nb.de/standards/xmetadissplus/"
    },
    {
      "prefix": "dc",
      "url": "http://purl.org/dc/elements/1.1/"
    },
    {
      "prefix": "dcterms",
      "url": "http://purl.org/dc/terms/"
    },
    {
      "prefix": "xsi",
      "url": "http://www.w3.org/2001/XMLSchema-instance"
    },
    {
      "prefix": "dini",
      "url": "http://www.d-nb.de/standards/xmetadissplus/type/"
    },
    {
      "prefix": "oai_dc",
      "url": "http://www.openarchives.org/OAI/2.0/oai_dc/"
    },
    {
      "prefix": "ddb",
      "url": "http://www.d-nb.de/standards/ddb/"
    }
  ],
  "dissTerms": [
    {
      "diss": "xDDC",
      "terms": [
        {
          "name": "xmetadissplus",
          "term": "//xMetaDiss:xMetaDiss/dc:subject[@xsi:type='dcterms:DDC' and .='$val']"
        },
        {
          "name": "oai_dc",
          "term": "//oai_dc:dc/dc:subject[.='info:eu-repo/classification/ddc/$val']"
        }
      ]
    },
    {
      "diss": "xOpenAccess",
      "terms": [
        {
          "name": "xmetadissplus",
          "term": "//xMetaDiss:xMetaDiss[dc:rights='info:eu-repo/semantics/openAccess']"
        },
        {
          "name": "oai_dc",
          "term": "//oai_dc:dc[dc:rights='info:eu-repo/semantics/openAccess']"
        }
      ]
    },
    {
      "diss": "xDocType",
      "terms": [
        {
          "name": "xmetadissplus",
          "term": "//xMetaDiss:xMetaDiss/dc:type[@xsi:type='dini:PublType' and .='$val']"
        },
        {
          "name": "oai_dc",
          "term": "//oai_dc:dc/dc:type='doc-type:$val'"
        }
      ]
    },
    {
      "diss": "xStatusType",
      "terms": [
        {
          "name": "xmetadissplus",
          "term": "//xMetaDiss:xMetaDiss/dini:version_driver='$val'"
        },
        {
          "name": "oai_dc",
          "term": "//oai_dc:dc/dc:type='info:eu-repo/semantics/$val'"
        }
      ]
    },
    {
      "diss": "xOpenAire",
      "terms": [
        {
          "name": "xmetadissplus",
          "term": "//xMetaDiss:xMetaDiss[dc:rights='info:eu-repo/semantics/openAccess' or dc:relation='info:eu-repo/grantAgreement/*']"
        },
        {
          "name": "oai_dc",
          "term": "//oai_dc:dc[dc:rights='info:eu-repo/semantics/openAccess' or dc:relation='info:eu-repo/grantAgreement/*']"
        }
      ]
    },
    {
      "diss": "xSourceSwb",
      "terms": [
        {
          "name": "xmetadissplus",
          "term": "//xMetaDiss:xMetaDiss[ddb:identifier[@ddb:type='Erstkat-ID']]"
        }
      ]
    }
  ]
